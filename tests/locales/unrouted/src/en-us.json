{

	/** Notation Test */

    /**
     * Phrase Notation
     */

    //Basic phrase
    "Hello": "Hello",
    //Basic phrase with sprintf
    "Hello %d": "Hello %d",
    //Basic phrase with interpolation
    "Hello {{name}}": "Hello {{name}}",
    //Advanced phrase with sprintf
    "Hello %d, my name is %d": "Hello %d, my name is %d",
    //Advanced phrase with interpolation
    "Hello {{name}}, my name is {{my.firstname}} {{my.lastname}}": "Hello {{name}}, my name is {{my.firstname}} {{my.lastname}}",

    /**
     * Bracket Notation
     */

    //Basic bracket
    "Hello": {
    	"default":"Hello"
    },
    //Basic bracket with dot key
    "greeting.informal":{
    	"default": "Hey"
    },
    //Advanced bracket with dot key
    "greeting.informal":{
    	"hey":{
    		"default":"Hey"
    	}
    },

    /**
     * Dot Notation
     */
    
    //Normal (nothing basic or advanced) dot
    "greeting":{
    	"informal":"Hey"
    },

    /** Plural Test */

    "There is one monkey in the %%s": {
    	// You can use this like so: __("There is one monky in the %%s", __("[There is one monkey in the %%s].tree"))
    	// Of course it's best to use dot notation to reduce the length
    	"tree":"tree", // <-- Notice this awesome hack?
        "one": "There is one monkey in the %%s",
        "other": "There are %d monkeys in the %%s"
    }

}