{

	/** Notation Test */

    /**
     * Phrase Notation
     */

    //Basic phrase
    "Hello": "こんにちは",
    //Basic phrase with sprintf
    "Hello %d": "こんにちは%d",
    //Basic phrase with interpolation
    "Hello {{name}}": "こんにちは{{name}}さん",
    //Advanced phrase with sprintf
    "Hello %d, my name is %d": "こんにちは%dさん, 私の名前は%dです",
    //Advanced phrase with interpolation
    "Hello {{name}}, my name is {{my.firstname}} {{my.lastname}}": "こんにちは{{name}}さん, 私の名前は{{my.firstname}}{{my.lastname}}です",

    /**
     * Bracket Notation
     */

    //Basic bracket
    "Hello": {
    	"default":"こんにちは"
    },
    //Basic bracket with dot key
    "greeting.informal":{
    	"default": "おっす"
    },
    //Advanced bracket with dot key
    "greeting.informal":{
    	"hey":{
    		"default":"おっす"
    	}
    },

    /**
     * Dot Notation
     */
    
    //Normal (nothing basic or advanced) dot
    "greeting":{
    	"informal":"おっす"
    },

    /** Plural Test */

    "There is one monkey in the %%s": {
    	// You can use this like so: __("There is one monky in the %%s", __("[There is one monkey in the %%s].tree"))
    	// Of course it's best to use dot notation to reduce the length
    	"tree":"木", // <-- Notice this awesome hack?
        "other": "一匹のサルが%%sにいます。"
    }

}