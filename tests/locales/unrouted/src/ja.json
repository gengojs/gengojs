{

  /** Notation Test */

  /**
   * Phrase Notation
   */

  // Basic phrase
  "Hello": "こんにちは",
  // Basic phrase with sprintf
  "Hello %s": "こんにちは%sさん",
  // Basic phrase with interpolation
  "Hello {{name}}": "こんにちは{{name}}さん",
  // Advanced phrase with sprintf
  "Hello %s, my name is %s": "こんにちは%sさん、 私の名前は%sです",
  // Advanced phrase with interpolation
  "Hello {{name}}, my name is {{my.firstname}} {{my.lastname}}": "こんにちは{{name}}さん、 私の名前は{{my.firstname}} {{my.lastname}}です",

  /**
   * Bracket Notation
   */

  // Basic bracket
  // Use [Hello]

  // Basic bracket with dot key
  "greeting.informal.basic": {
    "translated": "おっす"
  },
  // Advanced bracket with dot key
  "greeting.informal.advanced": {
    "hey": {
      "translated": "おっす"
    }
  },

  /**
   * Dot Notation
   */

  // Normal (nothing basic or advanced) dot
  "greeting": {
    "informal": "おっす"
  },

  /* MessageFormat Test*/

  "msgformat": {
    "photos": "あなたは{numPhotos,plural,=0{写真を持っていません。}=1{写真を持っています。}other{# 枚の写真をもっています。}}"
  }

}